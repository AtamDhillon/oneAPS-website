{
  "name": "oneAPS",
  "version": "0.0.1",
  "description": "Awesome project developed with TypeORM.",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/connect-redis": "^0.0.16",
    "@types/express": "^4.17.11",
    "@types/express-handlebars": "^3.1.0",
    "@types/express-session": "^1.17.3",
    "@types/ioredis": "^4.22.0",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.35",
    "@types/node-fetch": "^2.5.8",
    "@types/uuid": "^8.3.0",
    "@types/yup": "^0.29.11",
    "autoprefixer": "^9.4.10",
    "jest": "^26.6.3",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.7",
    "onchange": "^7.1.0",
    "postcss-cli": "^8.3.1",
    "request-promise": "^4.2.5",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "4.2.3"
  },
  "dependencies": {
    "@gov.au/accordion": "latest",
    "@gov.au/animate": "latest",
    "@gov.au/body": "latest",
    "@gov.au/breadcrumbs": "latest",
    "@gov.au/buttons": "latest",
    "@gov.au/callout": "latest",
    "@gov.au/card": "latest",
    "@gov.au/control-input": "latest",
    "@gov.au/core": "latest",
    "@gov.au/cta-link": "latest",
    "@gov.au/direction-links": "latest",
    "@gov.au/footer": "latest",
    "@gov.au/form": "latest",
    "@gov.au/grid-12": "latest",
    "@gov.au/header": "latest",
    "@gov.au/headings": "latest",
    "@gov.au/inpage-nav": "latest",
    "@gov.au/keyword-list": "latest",
    "@gov.au/link-list": "latest",
    "@gov.au/main-nav": "latest",
    "@gov.au/page-alerts": "latest",
    "@gov.au/progress-indicator": "latest",
    "@gov.au/responsive-media": "latest",
    "@gov.au/searchbox": "latest",
    "@gov.au/select": "latest",
    "@gov.au/side-nav": "latest",
    "@gov.au/skip-link": "latest",
    "@gov.au/table": "latest",
    "@gov.au/tags": "latest",
    "@gov.au/text-inputs": "latest",
    "@govau-platforms/notify-client": "^5.2.4",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cfenv": "^1.2.3",
    "connect-redis": "^5.1.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-handlebars": "^5.2.1",
    "express-session": "^1.17.1",
    "helmet": "^4.4.1",
    "ioredis": "^4.24.2",
    "node-fetch": "^2.6.1",
    "pg": "^8.4.0",
    "reflect-metadata": "^0.1.10",
    "typeorm": "0.2.31",
    "uuid": "^8.3.2",
    "yup": "^0.32.9"
  },
  "scripts": {
    "build:autoprefix": "postcss static/css/designsystem.min.css -u autoprefixer -r",
    "build:css": "node-sass --output-style compressed \"./src/sass/main.scss\" \"./static/css/designsystem.min.css\" ",
    "test": "NODE_ENV=test jest --force-exit --coverage --verbose false --runInBand --detectOpenHandles",
    "watch:css": "onchange \"./src/sass/main.scss\" -- npm run build:css && npm run build:autoprefix",
    "watch": "NODE_ENV=development nodemon --exec ts-node src/index.ts & npm run watch:css"
  },
  "pancake": {
    "auto-save": true,
    "plugins": true,
    "ignore": [],
    "json": {
      "enable": false,
      "location": "pancake/",
      "name": "pancake",
      "content": {
        "name": true,
        "version": true,
        "dependencies": true,
        "path": true,
        "settings": true
      }
    },
    "css": {
      "minified": true,
      "modules": false,
      "browsers": [
        "last 2 versions",
        "ie 8",
        "ie 9",
        "ie 10"
      ],
      "location": false,
      "name": false
    },
    "sass": {
      "modules": false,
      "location": "src/sass/auds",
      "name": "auds.scss"
    },
    "react": {
      "location": false
    },
    "js": {
      "minified": true,
      "modules": false,
      "location": "static/javascript/",
      "name": "designsystem.min.js"
    }
  },
  "jest": {
    "globalSetup": "./setup-tests.js",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
